#include <vector>
#include <unordered_map>
using namespace std;

class Solution {
public:
    int findMaxLength(vector<int>& a) {
        unordered_map<int,int> m;
        m[0]=-1;
        int s=0,ans=0;
        for(int i=0;i<a.size();i++){
            s+=a[i]==1?1:-1;
            if(m.count(s)) ans=max(ans,i-m[s]);
            else m[s]=i;
        }
        return ans;
    }
};

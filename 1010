#include <vector>
using namespace std;
class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        vector<int> remainderCount(60, 0);
        int count = 0;

        for (int t : time) {
            int rem = t % 60;
            int complement = (60 - rem) % 60;
            count += remainderCount[complement];
            remainderCount[rem]++;
        }
        return count;
    }
};
